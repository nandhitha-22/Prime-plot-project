        <h4>EMI Calculator</h4>
        <input type="number" id="principal" placeholder="Loan Amount (‚Çπ)" />
        <input type="number" id="rate" placeholder="Interest Rate (%)" />
        <input type="number" id="months" placeholder="Tenure (Months)" />
        <button onclick="calculateEMI()">Calculate EMI</button>
        <div class="emi-result" id="emiResult"></div>
    </div>

    <script>
        function toggleEMI() {
            const popup = document.getElementById("emiCalculator");
            popup.style.display = popup.style.display === "flex" ? "none" : "flex";
        }

        function calculateEMI() {
            const principalInput = document.getElementById("principal").value.trim();
            const rateInput = document.getElementById("rate").value.trim();
            const monthsInput = document.getElementById("months").value.trim();
            const resultBox = document.getElementById("emiResult");

            const P = parseFloat(principalInput);
            const annualRate = parseFloat(rateInput);
            const N = parseInt(monthsInput);

            // Validate input
            if (isNaN(P) || isNaN(annualRate) || isNaN(N) || P <= 0 || annualRate <= 0 || N <= 0) {
                resultBox.innerHTML = `<p style="color: red;">‚ùó Please enter valid positive numbers in all fields.</p>`;
                return;
            }

            const R = annualRate / 12 / 100;

            const EMI = (P * R * Math.pow(1 + R, N)) / (Math.pow(1 + R, N) - 1);
            const totalPayment = EMI * N;
            const totalInterest = totalPayment - P;

            resultBox.innerHTML = `
              <p>üìÖ Monthly EMI: ‚Çπ${EMI.toFixed(2)}</p>
              <p>üí∏ Total Interest: ‚Çπ${totalInterest.toFixed(2)}</p>
              <p>üí∞ Total Payment: ‚Çπ${totalPayment.toFixed(2)}</p>
            `;
        }
    </script>
