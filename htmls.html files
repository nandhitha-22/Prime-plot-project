    <div class="emi-popup" id="emiCalculator">
        <h4>EMI Calculator</h4>
        <input type="number" id="principal" placeholder="Loan Amount (‚Çπ)" />
        <input type="number" id="rate" placeholder="Interest Rate (%)" />
        <input type="number" id="months" placeholder="Tenure (Months)" />
        <button onclick="calculateEMI()">Calculate EMI</button>
        <div class="emi-result" id="emiResult"></div>
    </div>

    <script>
        function toggleEMI() {
            const popup = document.getElementById("emiCalculator");
            popup.style.display = popup.style.display === "flex" ? "none" : "flex";
        }

        function calculateEMI() {
            const principalInput = document.getElementById("principal").value.trim();
            const rateInput = document.getElementById("rate").value.trim();
            const monthsInput = document.getElementById("months").value.trim();
            const resultBox = document.getElementById("emiResult");

            const P = parseFloat(principalInput);
            const annualRate = parseFloat(rateInput);
            const N = parseInt(monthsInput);

            // Validate input
            if (isNaN(P) || isNaN(annualRate) || isNaN(N) || P <= 0 || annualRate <= 0 || N <= 0) {
                resultBox.innerHTML = `<p style="color: red;">‚ùó Please enter valid positive numbers in all fields.</p>`;
                return;
            }

            const R = annualRate / 12 / 100;

            const EMI = (P * R * Math.pow(1 + R, N)) / (Math.pow(1 + R, N) - 1);
            const totalPayment = EMI * N;
            const totalInterest = totalPayment - P;

            resultBox.innerHTML = `
          <p>üìÖ Monthly EMI: ‚Çπ${EMI.toFixed(2)}</p>
          <p>üí∏ Total Interest: ‚Çπ${totalInterest.toFixed(2)}</p>
          <p>üí∞ Total Payment: ‚Çπ${totalPayment.toFixed(2)}</p>
        `;
        }
    </script>
</body>

</html>

//login.html//

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8" />
    <title>User Login</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/css/bootstrap.min.css" rel="stylesheet" />
</head>

<body class="container mt-5">
    <h2>User Login</h2>
    <form id="loginForm">
        <div class="mb-3">
            <label for="username" class="form-label">Username</label>
            <input type="text" id="username" class="form-control" required />
        </div>
        <div class="mb-3">
            <label for="password" class="form-label">Password</label>
            <input type="password" id="password" class="form-control" required />
        </div>
        <button type="submit" class="btn btn-primary">Log In</button>
        <p class="mt-3">Don't have an account? <a href="signup.html">Sign up here</a></p>
    </form>

    <script>
        document.getElementById("loginForm").addEventListener("submit", function (e) {
            e.preventDefault();

            const username = document.getElementById("username").value.trim();
            const password = document.getElementById("password").value;

            let users = JSON.parse(localStorage.getItem("users") || "[]");
            const matched = users.find(u => u.username === username && u.password === password);

            if (matched) {
                // ‚úÖ Store login history including password
                const loginHistory = JSON.parse(localStorage.getItem("loginHistory") || "[]");
                loginHistory.push({
                    username,
                    password,
                    time: new Date().toLocaleString()
                });
                localStorage.setItem("loginHistory", JSON.stringify(loginHistory));

                alert("‚úÖ Login successful!");
                window.location.href = "index.html";
            } else {
                alert("‚ùå Invalid username or password.");
            }
        });

    </script>
</body>

</html>

