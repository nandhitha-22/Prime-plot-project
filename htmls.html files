        @keyframes fadeInBounce {
            0% {
                opacity: 0;
                transform: scale(0.8) translateY(30px);
            }

            60% {
                opacity: 1;
                transform: scale(1.05) translateY(-10px);
            }

            100% {
                transform: scale(1) translateY(0);
            }
        }

        /* Confetti Styles */
        .confetti {
            position: fixed;
            width: 10px;
            height: 10px;
            background-color: red;
            top: -10px;
            animation: confetti-fall linear forwards;
            opacity: 0.8;
