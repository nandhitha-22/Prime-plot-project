    <script>
        function loadPlots() {
            const container = document.getElementById("plotContainer");
            const storedPlots = JSON.parse(localStorage.getItem("galleryImages")) || [];

            storedPlots.forEach((plot) => {
                const col = document.createElement("div");
                col.className = "col-md-4";

                const card = document.createElement("div");
                card.className = "card plot-card";

                const img = document.createElement("img");
                img.src = plot.image;
                img.className = "card-img-top";
                img.alt = "Plot Image";

                const body = document.createElement("div");
                body.className = "card-body";

                const title = document.createElement("h5");
                title.className = "card-title";
                title.textContent = plot.title || `Plot Name: ${plot.plotName}`;

                const location = document.createElement("p");
                location.className = "location icon-text";
                location.innerHTML = `<i class="bi bi-geo-alt-fill"></i> ${plot.location}`;

                const size = document.createElement("p");
                size.className = "size icon-text";
                size.innerHTML = `<i class="bi bi-rulers"></i> ${plot.size}`;

                const price = document.createElement("p");
                price.className = "price";
                price.textContent = plot.price;

                const btn = document.createElement("a");
                btn.href = `plot-details.html?id=${storedPlots.indexOf(plot)}`;
                btn.className = "btn btn-primary";
                btn.textContent = "View Details";


                body.appendChild(title);
                body.appendChild(location);
                body.appendChild(size);
                body.appendChild(price);
                body.appendChild(btn);

                card.appendChild(img);
                card.appendChild(body);
                col.appendChild(card);
                container.appendChild(col);
            });
        }

        window.onload = loadPlots;
    </script>

