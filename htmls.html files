<body>

    <!-- Thank you card -->
    <div class="thank-you-box">
        <i class="fa-solid fa-circle-check"></i>
        <h1>Thank You!</h1>
        <p>Your message has been successfully sent. We will contact you very soon!</p>
        <a href="index.html">Back to Home</a>
    </div>

    <!-- Confetti Script -->
    <script>
        function createConfetti() {
            const confetti = document.createElement('div');
            confetti.classList.add('confetti');
            confetti.style.left = Math.random() * window.innerWidth + 'px';
            confetti.style.backgroundColor = `hsl(${Math.random() * 360}, 70%, 60%)`;
            confetti.style.animationDuration = (Math.random() * 2 + 3) + 's';
            confetti.style.transform = `rotate(${Math.random() * 360}deg)`;
            document.body.appendChild(confetti);
            setTimeout(() => {
                confetti.remove();
            }, 5000);
        }

        // Generate multiple confetti
        for (let i = 0; i < 80; i++) {
            setTimeout(createConfetti, i * 100);
        }
    </script>

</body>

</html>

//thanks.html//

<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <title>Thank You</title>
</head>

<body style="text-align:center; font-family:sans-serif; padding:50px;">
    <h2>‚úÖ Payment Successful!</h2>
    <p>Your booking is confirmed. We will contact you shortly.</p>
    <a href="plots.html">Back to Plot List</a>

    <script>
        // Mark the latest booking as "Paid"
        const bookings = JSON.parse(localStorage.getItem("bookings") || "[]");

        if (bookings.length > 0) {
            bookings[bookings.length - 1].paymentStatus = "Paid";
            localStorage.setItem("bookings", JSON.stringify(bookings));
        }
    </script>
</body>

</html>

