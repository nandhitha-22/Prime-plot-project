    <script>

        function getQueryParam(param) {
            const urlParams = new URLSearchParams(window.location.search);
            return urlParams.get(param);
        }

        function loadPlotDetails() {
            const id = getQueryParam('id');
            const plots = JSON.parse(localStorage.getItem('galleryImages')) || [];
            const plot = plots[id];

            if (!plot) {
                document.getElementById('details').innerHTML = '<p class="text-danger">Plot not found.</p>';
                return;
            }

            let detailHTML = `
            <img src="${plot.image}" class="plot-img mb-4" alt="Plot Image">
                <h1 id="plotName" class="title">${plot.title || `${plot.plotName}`}</h1>
                <div class="icon-text"><i class="bi bi-geo-alt-fill"></i> ${plot.location}</div>
                <div class="icon-text"><i class="bi bi-rulers"></i> ${plot.size}</div>
                <p class="price mt-3">${plot.price}</p>
                <p class="mt-4">This plot is located in a prime area with excellent access and visibility.</p>
                `;

            if (plot.ecDocument) {
                detailHTML += `
        <a href="${plot.ecDocument}" target="_blank" class="btn btn-outline-secondary mt-3">
          üìÑ View EC Document (${plot.ecFileName || 'Open'})
        </a>
      `;
            }

            document.getElementById('details').innerHTML = detailHTML;
        }

        window.onload = loadPlotDetails;
    </script>
    <div style="
        max-width: 500px;
        margin: auto;
